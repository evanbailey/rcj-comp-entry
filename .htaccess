DirectoryIndex default.php default.htm default.html 
ErrorDocument 404 /page-not-found.htm
ErrorDocument 403 /page-not-found.htm
Options -Indexes

<Files ~ "\.log$">
  Order allow,deny
  Deny from all
</Files>

RewriteEngine On 
RewriteCond %{SERVER_PORT} 80 
RewriteRule ^(.*)$ https://rcja.clubengineer.org/$1 [R,L]

